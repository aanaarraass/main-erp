<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="wizard_string_label_change" model="ir.ui.view">
            <field name="name">Change Name</field>
            <field name="model">wizard.terminology</field>
            <field name="arch" type="xml">
                <form string="Change Name">
                    <group>
                        <field name="terminology_setting_id"/>
                    </group>
                    <footer>
                        <button name="change_name" string="Change" type="object" class="btn-primary"/>
                        <button special="cancel" string="Cancel" class="btn-secondary"/>
                    </footer>
                </form>
            </field>
        </record>

        <record id="action_wizard_string_label_change" model="ir.actions.act_window">
            <field name="name">Change Name</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">wizard.terminology</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>
        <!--        <menuitem action="action_wizard_lang_export" id="menu_wizard_lang_export" parent="menu_translation_export"/>-->
    </data>
</odoo>
